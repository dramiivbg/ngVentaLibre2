{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths":{

      "@app/*": ["src/app/*"],
      "@admin/*": ["src/app/components/admin/*"],
      "@pages/*": ["src/app/components/pages/*"],
      "@auth/*": ["src/app/components/auth/*"],
      "@env/*": ["src/environments/*"],
      
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
